{
  "name": "case-booking-app",
  "version": "1.3.0",
  "cacheVersion": "1.0.10",
  "homepage": "/",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.50.5",
    "@tanstack/react-query": "^5.87.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "bcryptjs": "^3.0.2",
    "pg": "^8.16.3",
    "playwright": "^1.55.0",
    "react": "^18.2.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-scripts": "5.0.1",
    "serve": "^14.2.4",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "NODE_OPTIONS='--max-old-space-size=8192' react-scripts start",
    "build": "react-scripts build",
    "build:enterprise": "node scripts/build-enterprise.js",
    "build:production": "npm run build:enterprise && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "typecheck": "npx tsc --noEmit",
    "quick-build": "npm run build 2>&1 | head -20",
    "claude-help": "source scripts/claude-helpers.sh && show_menu",
    "component-summary": "source scripts/claude-helpers.sh && component_summary",
    "ts-check": "source scripts/claude-helpers.sh && ts_check",
    "size-analysis": "source scripts/claude-helpers.sh && size_analysis",
    "claude-optimize": "chmod +x scripts/token-optimizer.sh && ./scripts/token-optimizer.sh && node scripts/claude-speed-optimizer.js",
    "claude-overview": "node scripts/claude-speed-optimizer.js --overview-only",
    "claude-analyze": "find src -name '*.tsx' -o -name '*.ts' -o -name '*.css' | xargs wc -c | sort -nr",
    "claude-clean": "rm -rf .claude-optimized",
    "claude-report": "cat .claude-optimized/size-report.md 2>/dev/null || echo 'Run npm run claude-optimize first'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "cache-version": "node -e \"const fs=require('fs'); const pkg=JSON.parse(fs.readFileSync('package.json')); const [major,minor,patch]=pkg.cacheVersion.split('.').map(Number); pkg.cacheVersion=major+'.'+minor+'.'+(patch+1); fs.writeFileSync('package.json',JSON.stringify(pkg,null,2)+'\\n'); console.log('Cache version updated to:',pkg.cacheVersion)\"",
    "test:concurrent": "npx ts-node src/scripts/runConcurrentTests.ts",
    "test:concurrent:quick": "npx ts-node -e \"import('./src/utils/concurrentUserTester').then(m => m.runQuickConcurrentTest())\"",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "npm run typecheck && npm run lint && npm test -- --coverage --watchAll=false",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "pre-commit": "npm run validate && npm run test:coverage",
    "claude:test": "node .claude/claude-test.js",
    "claude:quick": "node .claude/claude-test.js quick",
    "claude:security": "node .claude/claude-test.js security",
    "claude:build": "node .claude/claude-test.js build",
    "claude:full": "node .claude/claude-test.js full",
    "test:e2e": "npx playwright test --config=.claude/playwright.config.js",
    "test:e2e:headed": "npx playwright test --headed",
    "verify-app": "npm run claude:quick",
    "production-ready": "npm run claude:full && echo 'âœ… App verified as production ready!'",
    "audit:comprehensive": "node scripts/comprehensive-audit.js",
    "health-check": "node scripts/full-application-check.js",
    "test:db": "node scripts/test-all-fixes.js",
    "check:all": "npm run audit:comprehensive && npm run health-check && npm run test:db"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@playwright/test": "^1.55.0",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@testing-library/jest-dom": "^6.8.0",
    "cypress": "^15.2.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^6.1.0",
    "gh-pages": "^6.3.0",
    "msw": "^1.3.5",
    "prettier": "^3.6.2",
    "puppeteer": "^24.22.0",
    "react-app-rewired": "^2.2.1",
    "supabase": "^2.31.4"
  }
}
